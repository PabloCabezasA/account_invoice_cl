          <para style="terp_default_9b">[[ te['descripcion'] ]]</para>
        </td>
        <td>
          <para style="texto4">[[ te['cantidad'] ]]</para>
        </td>
        <td>
          <para style="texto5">[[ te['valor'] ]]</para>
        </td>
      </tr>
    </blockTable>
	
	
			<section>
